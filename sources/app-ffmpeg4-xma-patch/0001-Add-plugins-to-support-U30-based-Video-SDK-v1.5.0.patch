+        else
+        {
+            av_log(ctx, AV_LOG_ERROR, "decoder error: %s : ffmpeg pid %d on device index =  %d cu index = %d\n",
+                   err_str, getpid(), ctx->decode_cu_list_res.cuResources[0].deviceId,
+                   ctx->decode_cu_list_res.cuResources[1].cuId);
+        }
+    }
+
+    return err_type;
