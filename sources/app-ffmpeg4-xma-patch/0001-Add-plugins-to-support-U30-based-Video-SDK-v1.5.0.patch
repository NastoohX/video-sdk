+/**
+ * @file
+ * video Multi Scaler IP (in ABR mode) with Xilinx Media Accelerator
+ */
+
+#include <stdio.h>
+#include <unistd.h>
+#include <xma.h>
+#include <xmaplugin.h>
+#include <xrm.h>
