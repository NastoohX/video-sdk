+static av_cold void multiscale_xma_uninit(AVFilterContext *ctx)
+{
+    int i;
+    MultiScalerContext *s = ctx->priv;
+
+#ifdef DUMP_OUT_FRAMES
+    fclose (outfp);
+    fclose (yfp);
