+static int64_t
+mpsoc_vcu_encode_dequeue_pts(AVFifoBuffer* queue)
+{
+    int64_t ts = AV_NOPTS_VALUE;
+    if (av_fifo_size(queue) > 0)
+        av_fifo_generic_read(queue, &ts, sizeof(ts), NULL);
+    return ts;
+}
