+    for (i = 0; i < ctx->nb_outputs; i++)
+        av_freep(&ctx->output_pads[i].name);
+
+       for (int idx=0; idx < s->num_sessions; idx++) {
+          if (s->session[idx])
+            xma_scaler_session_destroy(s->session[idx]);
